                                        <div class="text-documents">
                                            <span><i class='bx bxs-file-pdf'></i>
                                                <%= certificado.name %>
                                            </span>

                                            <a href="/mostrar_ae/<%= certificado %>"> <i class='bx bx-plus'></i> Mais
                                                informações </a>
                                        </div>
                                    </div>
                                    <% }}); %>
                        </div>

                        <button class="accordion">
                            <i class="bx bxs-folder-open"></i>Prestação de Serviço
                        </button>
                        <div class="panel">
                            <% certificado.forEach(function(certificado){ %>
                                <% if (name_subcategory=='Prestação de Serviço' ) {%>
                                    <div class="panel-documentos">
                                        <div class="text-documents">
                                            <span><i class='bx bxs-file-pdf'></i>
                                                <%= certificado.name %>
                                            </span>

                                            <a href="/mostrar_ae/<%= certificado %>"> <i class='bx bx-plus'></i> Mais
                                                informações </a>
                                        </div>
                                    </div>
                                    <% }}); %>
                        </div>

                    </div>

                </div>
            </div>
            </div>
            </div>
        </section>
        </main>
        <script src="/public/js/javascript.js"></script>
        </body>

        </html>