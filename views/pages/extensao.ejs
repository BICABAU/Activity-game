<%- contentFor('title') %>
    Atividades de Extensão

    <%- contentFor('main') %>
        <section class="atividades" id="main">
            <div class="atividades-content">
                <div class="conteudo-container">
                    <div class="lista-atividades">
                        <div class="atividades-titulos">
                            <h2>Atividades de Extensão</h2>
                            <div class="btn-box">
                                <a href="/postAEs"><button>Registre uma nova atividade</button></a>
                            </div>
                        </div>

                        <button class="accordion">
                            <i class="bx bxs-folder-open"></i>Programa
                        </button>
                        <div class="panel">
                            <% certificado.forEach(function(certificado){ %>
                                <% if (name_subcategory=='Programa' ) {%>
                                    <div class="panel-documentos">
                                        <div class="text-documents">
                                            <span><i class='bx bxs-file-pdf'></i>
                                                <%= certificado.name %>
                                            </span>
                                            <span>
                                                <%= certificado.criado_em %>
                                            </span>
                                            <a href="/mostrar_ae/<%= certificado %>"> <i class='bx bx-plus'></i> Mais
                                                informações </a>
                                        </div>
                                    </div>
                                    <% }}); %>


                        </div>

                        <button class="accordion">
                            <i class="bx bxs-folder-open"></i>Projeto
                        </button>
                        <div class="panel">
                            <% certificado.forEach(function(certificado){ %>
                                <% if (name_subcategory=='Projeto' ) {%>
                                    <div class="panel-documentos">
                                        <div class="text-documents">
                                            <span><i class='bx bxs-file-pdf'></i>
                                                <%= certificado.name %>
                                            </span>

                                            <a href="/mostrar_ae/<%= certificado %>"> <i class='bx bx-plus'></i> Mais
                                                informações </a>
                                        </div>
                                    </div>
                                    <% }}); %>
                        </div>

                        <button class="accordion">
                            <i class="bx bxs-folder-open"></i>Curso
                        </button>
                        <div class="panel">
                            <% certificado.forEach(function(certificado){ %>
                                <% if (name_subcategory=='Curso' ) {%>
                                    <div class="panel-documentos">
                                        <div class="text-documents">
                                            <span><i class='bx bxs-file-pdf'></i>
                                                <%= certificado.name %>
                                            </span>

                                            <a href="/mostrar_ae/<%= certificado %>"> <i class='bx bx-plus'></i> Mais
                                                informações </a>
                                        </div>
                                    </div>
                                    <% }}); %>
                        </div>

                        <button class="accordion">
                            <i class="bx bxs-folder-open"></i>Evento
                        </button>
                        <div class="panel">
                            <% certificado.forEach(function(certificado){ %>
                                <% if (name_subcategory=='Evento' ) {%>
